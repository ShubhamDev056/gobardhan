  
<IfModule mod_rewrite.c>
    RewriteEngine On
	Options All -Indexes
    RewriteBase /
    RewriteCond %{REQUEST_METHOD} !^(GET|POST)
    RewriteRule ^index\.php$ - [L]
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule . /gobardhan.co.in/index.php [L]

 </IfModule>
