.footer-instagram ul li a::before {
  position: absolute;
  background-color: #f38632;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  content: "";
  opacity: 0;
  -webkit-transition: all 0.3s ease 0s;
  -o-transition: all 0.3s ease 0s;
  transition: all 0.3s ease 0s;
}
.footer-instagram ul li a img {
  width: 100%;
